<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <!-- 自定义添加开始 -->
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/cssjqryUi")
    <!-- 自定义添加结束 -->

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        @*<a href="#" class="dropdown-toggle" data-toggle="dropdown">ss</a>*@
                        @Html.ActionLink("合同管理", "About", "Home", new { @class = "dropdown-toggle" }, new { data_toggle = "dropdown" })
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("合同查询", "QueryContract", "Contract")</li>
                            <li>@Html.ActionLink("合同添加", "AddContract", "Contract")</li>
                            <li>@Html.ActionLink("合同修改", "ModContract", "Contract")</li>
                            <li>@Html.ActionLink("合同删除", "DelContract", "Contract")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        @Html.ActionLink("桥梁信息管理", "About", "Home", new { @class = "dropdown-toggle" }, new { data_toggle = "dropdown" })
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("桥梁总览", "Index", "Bridge")</li>
                            <li>@Html.ActionLink("桥梁查询", "QueryBridge", "Bridge")</li>
                            <li>@Html.ActionLink("桥梁添加", "AddBridge", "Bridge")</li>
                            <li>@Html.ActionLink("桥梁修改", "ModBridge", "Bridge")</li>
                            <li>@Html.ActionLink("桥梁删除", "DelBridge", "Bridge")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        @Html.ActionLink("项目信息管理", "About", "Home", new { @class = "dropdown-toggle" }, new { data_toggle = "dropdown" })
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("最新项目", "Index", "Inspection_project")</li>
                            <li>@Html.ActionLink("项目查询", "QueryInspection_project", "Inspection_project")</li>
                            <li>@Html.ActionLink("项目添加", "AddInspection_project", "Inspection_project")</li>
                            <li>@Html.ActionLink("项目修改", "ModInspection_project", "Inspection_project")</li>
                            <li>@Html.ActionLink("项目删除", "DelInspection_project", "Inspection_project")</li>
                            <li>@Html.ActionLink("项目参与者信息添加", "AddR_inspection_project_staff", "R_bridge_inspection_staff")</li>
                            <li>@Html.ActionLink("项目产值查询", "Query_R_bridge_inspection_staff_By_staff_id", "R_bridge_inspection_staff")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        @Html.ActionLink("职工信息管理", "About", "Home", new { @class = "dropdown-toggle" }, new { data_toggle = "dropdown" })
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("职工信息总览", "Index", "Staff")</li>
                            <li>@Html.ActionLink("职工信息查询", "QueryStaff", "Staff")</li>
                            <li>@Html.ActionLink("职工信息添加", "AddStaff", "Staff")</li>
                            <li>@Html.ActionLink("职工信息修改", "ModStaff", "Staff")</li>
                            <li>@Html.ActionLink("职工信息删除", "Delstaff", "Staff")</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    @RenderSection("scripts", required: false)
</body>
</html>
